{
    "extends": ["fbjs", "prettier"],

    "plugins": ["prettier", "react"],

    "prettier/prettier": [
        "error",
        {
            "bracketSpacing": false,
            "jsxBracketSameLine": true,
            "singleQuote": true,
            "trailingComma": "es5",
            "printWidth": 100
        }
    ]
}